<% if @status == 'success' %>
$("#attr-sp-<%= @service_provider.id %>").replaceWith("<%= j(render(@template, service_provider: @service_provider)) %>");
$("#service_provider-modal").modal("hide");
<%  elsif @status == 'fail' %>
$("#modal_error").replaceWith("<%= j(render("modal_error", service_provider: @service_provider)) %>");
<% end %>
