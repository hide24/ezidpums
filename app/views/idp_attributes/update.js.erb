<% if @status == 'success' %>
$("#attr-ia-<%= @idp_attribute.id %>").replaceWith("<%= j(render("tr", idp_attribute: @idp_attribute)) %>");
$("#idp_attribute-modal").modal("hide");
<%  elsif @status == 'fail' %>
$("#modal_error").replaceWith("<%= j(render("modal_error", idp_attribute: @idp_attribute)) %>");
<% end %>
