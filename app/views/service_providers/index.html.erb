<p id="notice"><%= notice %></p>

<h1>Service Providers</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>EntityID</th>
      <th>Certification</th>
      <th>Enable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_providers.each do |service_provider| %>
      <%= render "tr", service_provider: service_provider %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service Provider', new_service_provider_path, remote: true, class: "btn btn-lg btn-primary" %>
<div id="service_provider-modal" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true"></div>
